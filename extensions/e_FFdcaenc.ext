<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
    <AudioEncoder Name="FFDCAENC DTS Encoder" UniqueID="3f1e0789-192a-4453-9d1a-66afd4c6b0c3">
        <Plugin>
            <MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>480</DialogWidth>
				<LogoBitmap></LogoBitmap>				
				<Url>http://forum.doom9.org/showthread.php?t=170952</Url>
				<UrlToolTip>Project Forum Page</UrlToolTip>
				<Script>32==Audiobits(last)?ConvertAudioTo32bit(last):last</Script>
				<ExecutableFileName>ffdcaenc_213-20141209.exe</ExecutableFileName>
				<ExecutableCommandline>-i - -o "{0}" -l %options%</ExecutableCommandline>
				<TitleFormatString>DTS FFDCAENC - {0}</TitleFormatString>
				<SupportedFileExtension>dts</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<UseRawPCM>false</UseRawPCM>
				<Info> the ACTUAL bitrate will always be a multiple of:\n
						3	kbps for	48 kHz
						2.75625	kbps for	44.1 kHz
						2	kbps for	32 kHz
						1.5	kbps for	24 kHz
						1.378125	kbps for	22.05 kHz
						1	kbps for	16 kHz
						0.75	kbps for	12 kHz
						0.6890625 kbps for	11.025 kHz
						0.5	kbps for	8 kHz
				</Info>
				<Radiobutton Name="Constant Bitrate {0} kb/s" Default="true">
					<Value>-b {0}</Value>
					<Trackbar DefaultValue="15" FixedValues="true">
						<Value>32</Value>
						<Value>56</Value>
						<Value>64</Value>
						<Value>96</Value>
						<Value>112</Value>
						<Value>128</Value>
						<Value>192</Value>
						<Value>224</Value>
						<Value>256</Value>
						<Value>320</Value>
						<Value>384</Value>
						<Value>448</Value>
						<Value>512</Value>
						<Value>576</Value>
						<Value>640</Value>
						<Value>755.50</Value>
						<Value>768</Value>
						<Value>960</Value>
						<Value>1024</Value>
						<Value>1152</Value>
						<Value>1280</Value>
						<Value>1344</Value>
						<Value>1408</Value>
						<Value>1411.20</Value>
						<Value>1472</Value>
						<Value>1509.75</Value>
						<Value>1536</Value>
						<Value>1920</Value>
						<Value>2048</Value>
						<Value>3072</Value>
						<Value>3840</Value>
					</Trackbar>
				</Radiobutton>
				<Radiobutton Name="Constant Float Bitrate {0:f2} kb/s">
					<Value>-b {0:f2}</Value>
					<Trackbar DefaultValue="15360">
						<Min>640</Min>
						<Max>76800</Max>
						<TickFrequency>1280</TickFrequency>
						<Multiplier>0.05</Multiplier>
					</Trackbar>
					<ToolTip>Constant Float Bitrate is needed for some sample rate input</ToolTip>
				</Radiobutton>
				<Dropdown Name="Byte-order">
					<Item Name="Big-Endian">
						<Value></Value></Item>
					<Item Name="Little-Endian">
						<Value>-e</Value></Item>
					<ToolTip>Sets the endianness. Default is 'Big-Endian'.</ToolTip>
				</Dropdown>
				<Dropdown Name="Channel configuration">
					<Item Name="Auto-selection">
						<Value></Value></Item>
					<Item Name="A">
						<Value>-c 1</Value></Item>
					<Item Name="A, B">
						<Value>-c 2</Value></Item>
					<Item Name="L, R">
						<Value>-c 3</Value></Item>
					<Item Name="(L+R), (L-R)">
						<Value>-c 4</Value></Item>
					<Item Name="Lt, Rt">
						<Value>-c 5</Value></Item>
					<Item Name="FC, FL, FR">
						<Value>-c 6</Value></Item>
					<Item Name="FL, FR, BC">
						<Value>-c 7</Value></Item>
					<Item Name="FC, FL, FR, BC">
						<Value>-c 8</Value></Item>
					<Item Name="FL, FR, BL, BR">
						<Value>-c 9</Value></Item>
					<Item Name="FC, FL, FR, BL, BR">
						<Value>-c 10</Value></Item>
					<ToolTip>Overrides the channel configuration.</ToolTip>
				</Dropdown>
				<Checkbox Name="Reduced Bit Depth">
					<ValueChecked>-r</ValueChecked>
					<ToolTip>Reduced Bit Depth for DTS CD format.</ToolTip>
				</Checkbox>
				<Checkbox Name="Add LFE channel">
					<ValueChecked>-f</ValueChecked>
					<ToolTip>By default an LFE channel will only be created with 6-channel input files.
							If checked, an LFE channel will be added to all other channel configurations as well.</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>
