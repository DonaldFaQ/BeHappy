<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="C20E261E-F31C-44F4-AE29-F181B6A70B08">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>550</DialogWidth>
				<Url>http://wiki.hydrogenaud.io/index.php?title=Fraunhofer_FDK_AAC#fdkaac</Url>
				<UrlToolTip>Encoder documentation and download.</UrlToolTip>
				<LogoBitmap></LogoBitmap>
				<Script>16==Audiobits(last)?last:ConvertAudioTo16bit(last)</Script>
				<ExecutableFileName>fdkaac.exe</ExecutableFileName>
				<ExecutableCommandline>%options% -I -S -o "{0}" -</ExecutableCommandline>
				<TitleFormatString>AAC FDK - {0}</TitleFormatString>
				<SupportedFileExtension>m4a</SupportedFileExtension>
				<SupportedFileExtension>aac</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<Radiobutton Name="CBR mode {0} kb/s" Default="true">
					<Value>-b {0}</Value>
					<Trackbar DefaultValue="130">
						<Min>8</Min>
						<Max>576</Max>
						<TickFrequency>32</TickFrequency>
					</Trackbar>
					<ToolTip>Constant Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="VBR mode {0}" >
					<Value>-m {0}</Value>
					<Trackbar DefaultValue="3">
						<Min>1</Min>
						<Max>5</Max>
					</Trackbar>
					<ToolTip>VBR mode is not officially supported, and works only on a certain combination\nof parameter settings, sample rate, and channel configuration.\n
							 Per channel bitrate:
								Mono  /  Stereo
								VBR 1:  20  /  32 kbps
								VBR 2:  32  /  40 kbps
								VBR 3:  48  /  56 kbps
								VBR 4:  64  /  72 kbps
								VBR 5:  96  /  112 kbps
					</ToolTip>
				</Radiobutton>
				<Dropdown Name="AAC Profile">
					<Item Name="MPEG-4 AAC LC">
						<Value></Value></Item>
					<Item Name="MPEG-4 HE-AAC(SBR)">
						<Value>-p 5</Value></Item>
					<Item Name="MPEG-4 HE-AAC v2(SBR+PS)">
						<Value>-p 29</Value></Item>
					<Item Name="MPEG-4 AAC ELD">
						<Value>-p 39</Value></Item>
					<Item Name="MPEG-2 AAC LC">
						<Value>-p 129</Value></Item>
					<Item Name="MPEG-2 HE-AAC(SBR)">
						<Value>-p 132</Value></Item>
					<Item Name="MPEG-2 HE-AAC v2(SBR+PS)">
						<Value>-p 156</Value></Item>
					<ToolTip>Force AAC Profile</ToolTip>
				</Dropdown>
				<Dropdown Name="Gapless Mode">
					<Item Name="iTunSMPB ">
						<Value></Value></Item>
					<Item Name="ISO standard">
						<Value>-G 1</Value></Item>
					<Item Name="Both">
						<Value>-G 2</Value></Item>
					<ToolTip>Encoder delay signaling for gapless playback</ToolTip>
				</Dropdown>
				<Dropdown Name="Transport format">
					<Item Name="RAW ">
						<Value></Value></Item>
					<Item Name="ADIF">
						<Value>-f 1</Value></Item>
					<Item Name="ADTS">
						<Value>-f 2</Value></Item>
					<Item Name="LATM MCP=1">
						<Value>-f 6</Value></Item>	
					<Item Name="LATM MCP=0">
						<Value>-f 7</Value></Item>
					<Item Name="LOAS/LATM">
						<Value>-f 10</Value></Item>
				</Dropdown>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>
