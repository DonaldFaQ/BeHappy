<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="862d8c17-e30c-4153-af72-e2c63721b546">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>400</DialogWidth>
				<Info>
                                Only allowed:
                                Stereo audio.
                                Int samples (8, 16, 24 or 32 bits).
				</Info>

				<Url>http://losslessaudio.org/</Url>
				<UrlToolTip>Encoder documentation and download.</UrlToolTip>
 <LogoBitmap></LogoBitmap>
				<Script>IsAudioFloat(last)?ConvertAudioTo24bit(last):last</Script>
				<TitleFormatString>OFR OptimFROG - {0}</TitleFormatString>
				<ExecutableFileName>ofr.exe</ExecutableFileName>
				<ExecutableCommandline>--encode %options% - --output "{0}"</ExecutableCommandline>
				<SupportedFileExtension>ofr</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<IsLossless>true</IsLossless>
				<Radiobutton Name="Max compression Preset">
					<Value>--preset max</Value>
					<ToolTip>Max Compression Preset</ToolTip>
				</Radiobutton>
				<Radiobutton Name="Compression Preset: {0}">
					<Value>--preset {0}</Value>
					<Trackbar DefaultValue="2">
						<Min>0</Min>
						<Max>10</Max>
						<TickFrequency>1</TickFrequency>
						<Multiplier>1</Multiplier>
					</Trackbar>
					<ToolTip>From low to high</ToolTip>
				</Radiobutton>
			</MultiOptionEncoder>
		</Plugin>
	</AudioEncoder>
</BeHappy.Extension>
