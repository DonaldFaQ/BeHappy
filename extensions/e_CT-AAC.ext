<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="e0d106be-88ca-481c-8db3-ac4c9bbc7c62">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>470</DialogWidth>
				<LogoBitmap></LogoBitmap>
				<Script>16==Audiobits(last)?last:ConvertAudioTo16bit(last)</Script>			<!-- Better an error than a wrong result when multichannel -->
				<ExecutableFileName>aacPlus\enc_aacplus.exe</ExecutableFileName>
				<ExecutableCommandline>- "{0}" --rawpcm {1} {3} {2} %options%</ExecutableCommandline>
				<TitleFormatString>AAC CT aacPlus - {0}</TitleFormatString>
				<SupportedFileExtension>mp4</SupportedFileExtension>
				<SupportedFileExtension>m4a</SupportedFileExtension>
				<SupportedFileExtension>3gp</SupportedFileExtension>
				<SupportedFileExtension>3g2</SupportedFileExtension>
				<SupportedFileExtension>aac</SupportedFileExtension>
				<Info>AACPlus encoder needs Winamp support files. Copy the needed files to " \BeHappy\encoder\aacPlus\* ".</Info>
				<Radiobutton Name="Constant Bitrate {0} kb/s">
					<Value>--br {0}000</Value>
					<Trackbar DefaultValue="128">
						<Min>8</Min>
						<Max>320</Max>
						<TickFrequency>12</TickFrequency>
					</Trackbar>
				</Radiobutton>
				<Dropdown Name="AAC Profile">
					<Item Name="Automatic">
						<Value></Value></Item>
					<Item Name="LC">
						<Value>--lc</Value></Item>
					<Item Name="HE">
						<Value>--he</Value></Item>
					<Item Name="HE High">
						<Value>--high</Value></Item>
					<ToolTip>Force AAC Profile:\n
						LC (mono:8-16, stereo: 16-320, 6ch: 160-320 kbps)
						HE (mono:8-64, stereo: 16-128, 6ch: 96-213 kbps)
						HE High (mono:8-160, stereo: 96-256, 6ch: 8-256 kbps)
					</ToolTip>
				</Dropdown>
				<Dropdown Name="Channel Mode">
					<Item Name="Joint Stereo">
						<Value></Value></Item>
					<Item Name="Independent Stereo">
						<Value>--is</Value></Item>
					<Item Name="Mono">
						<Value>--mono</Value></Item>
					<Item Name="Dual Channels">
						<Value>--dc</Value></Item>
					<ToolTip>Force Channel Mode</ToolTip>
				</Dropdown>
				<Checkbox Name="Enable PNS">
					<ValueChecked>--pns</ValueChecked>
					<ToolTip>Enable Perceptual Noise Subsitution</ToolTip>
				</Checkbox>
				<Checkbox Name="Tune for SPEECH">
					<ValueChecked>--speech</ValueChecked>
					<ToolTip>Tune for Speech</ToolTip>
				</Checkbox>
				<Checkbox Name="Force MPEG-4 AAC stream">
					<ValueChecked>--mpeg4aac</ValueChecked>
					<ToolTip>Force MPEG4 AAC(instead of let the encoder choose)</ToolTip>
				</Checkbox>	
				<Checkbox Name="Enable Parametric Stereo">
					<ValueChecked>--ps</ValueChecked>
					<ToolTip>bitrates up to 56000, only with HE-AAC</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>
		</Plugin>
	</AudioEncoder>
</BeHappy.Extension>
