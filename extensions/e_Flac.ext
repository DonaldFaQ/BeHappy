<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="9EFEC7A9-9081-4DC3-B801-1D5119997E66">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>500</DialogWidth>
				<Url>https://xiph.org/flac</Url>
				<LogoBitmap></LogoBitmap>
				<Script>16==Audiobits(last)?last:ConvertAudioTo24bit(last)</Script>
				<ExecutableFileName>flac.exe</ExecutableFileName>
				<ExecutableCommandline>%options% - --force --silent --force-raw-format --endian=little --channels={3} --bps={2} --sample-rate={1} --sign=signed  -o "{0}"</ExecutableCommandline>
				<TitleFormatString>FLAC - {0}</TitleFormatString>
				<SupportedFileExtension>flac</SupportedFileExtension>
				<SupportedFileExtension>fla</SupportedFileExtension>
				<SupportedFileExtension>ogg</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<UseRawPCM>true</UseRawPCM>
				<IsLossless>true</IsLossless>
				<Dropdown Name="Compressing Level" DefaultIndex="5">
					<Item Name="Fast">
						<Value>--fast</Value></Item>
					<Item Name="1">
						<Value>-1</Value></Item>
					<Item Name="2">
						<Value>-2</Value></Item>
					<Item Name="3">
						<Value>-3</Value></Item>
					<Item Name="4">
						<Value>-4</Value></Item>
					<Item Name="Default">
						<Value>-5</Value></Item>
					<Item Name="6">
						<Value>-6</Value></Item>
					<Item Name="7">
						<Value>-7</Value></Item>
					<Item Name="Best">
						<Value>-8</Value></Item>
					</Dropdown>
				<Checkbox Name="Warnings as errors">
					<ValueChecked>--warnings-as-errors</ValueChecked>
					<ToolTip>Treat all warnings as errors</ToolTip>
				</Checkbox>
				<Checkbox Name="LossyFLAC">
					<ValueChecked>-b 512 --keep-foreign-metadata</ValueChecked>
					<ToolTip>Set to frame size limit to 512k samples for perchannels</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>
