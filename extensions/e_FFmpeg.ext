	<AudioEncoder UniqueID="553c740c-9216-4296-947b-8a1fabdd0447">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>450</DialogWidth>
				<LogoBitmap></LogoBitmap>
				<Url>https://www.ffmpeg.org/</Url>
				<Script>32==Audiobits(last)?ConvertAudioTo32bit(last):last</Script>
				<ExecutableFileName>ffmpeg.exe</ExecutableFileName>
				<ExecutableCommandline>-i - -y -f mp4 %options% "{0}"</ExecutableCommandline>
				<TitleFormatString>AAC FFmpeg - {0}</TitleFormatString>
				<SupportedFileExtension>m4a</SupportedFileExtension>
				<SupportedFileExtension>mp4</SupportedFileExtension>
				<SupportedFileExtension>3gp</SupportedFileExtension>
				<SupportedFileExtension>3g2</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<Dropdown Name="AAC Encoder Library">
					<Item Name="Native">
						<Value>-c:a aac -strict -2</Value></Item>
					<Item Name="libFDK">
						<Value>-c:a libfdk_aac</Value></Item>
					<Item Name="libFAAC">
						<Value>-c:a libfaac</Value></Item>
					<ToolTip>LibFDK is the Fraunhofer AAC encoder library, developed for Android. LibFAAC is the Freeware Advanced Audio Coder.\n
							Both of them need to be explicit enabled in your FFmpeg build.</ToolTip>
				</Dropdown>
				<Radiobutton Name="Constant Bitrate {0} kb/s" Default="true">
					<Value>-b:a {0}k</Value>
					<Trackbar DefaultValue="16">
						<Min>1</Min>
						<Max>72</Max>
						<TickFrequency>4</TickFrequency>
						<Multiplier>8</Multiplier>
					</Trackbar>
					<ToolTip>Constant Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="VBR mode Quality: {0:f1}">
					<Value>-q:a {0:f1}</Value>
					<Trackbar DefaultValue="50">
						<Min>1</Min>
						<Max>100</Max>
						<TickFrequency>10</TickFrequency>
						<Multiplier>0.1</Multiplier>
					</Trackbar>
					<ToolTip>Variable Bitrate mode</ToolTip>
				</Radiobutton>	
			</MultiOptionEncoder>
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>
