<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="cc6ae8d8-8f35-403e-8226-4162fb783a99">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<Script>6==AudioChannels(last)?last:GetChannel(last,1,1,1,1,1,1)
					16==Audiobits(last)?last:ConvertAudioTo16bit(last)</Script>
				<DialogWidth>400</DialogWidth>
				<ExecutableFileName>enc_AudX_CLI.exe</ExecutableFileName>
				<TitleFormatString>Aud-X - {0}</TitleFormatString>
				<SupportedFileExtension>mp3</SupportedFileExtension>
				<UseRawPCM>true</UseRawPCM>
				<Option>
					<Name>STRQ (80 kbit/s mono + surr.)</Name>
					<Value>- "{0}" --raw {1} --q 0</Value>
				</Option>
				<Option default="true">
					<Name>STDQ (128 kbit/s mono + surr.)</Name>
					<Value>- "{0}" --raw {1} --q 1</Value>
				</Option>
				<Option>
					<Name>HGHQ (192 kbit/s dplII + surr.)</Name>
					<Value>- "{0}" --raw {1} --q 2</Value>
				</Option>
				<Option>
					<Name>SPBQ (192 kbit/s mono + surr.)</Name>
					<Value>- "{0}" --raw {1} --q 3</Value>
				</Option>
				<LogoBitmap></LogoBitmap>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>