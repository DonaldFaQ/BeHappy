<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="7930c6b7-8bd2-4fa8-b209-65377572aa24">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>350</DialogWidth>
				<Info>
				Quality 	Bitrate
				-------     -------
				-q0 	64 kb/s
				-q1 	80 kb/s
				-q2 	96 kb/s
				-q3 	112 kb/s
				-q4 	128 kb/s
				-q5 	160 kb/s
				-q6 	192 kb/s
				-q7 	224 kb/s
				-q8 	256 kb/s
				-q9 	320 kb/s
				-q10 	500 kb/s </Info>
				<LogoBitmap></LogoBitmap>
				<Url>http://www.vorbis.com</Url>
				<UrlToolTip></UrlToolTip>
				<Script>6==AudioChannels(last)?GetChannel(last,1,3,2,5,6,4):last</Script>
				<ExecutableFileName>oggenc2.exe</ExecutableFileName>
				<ExecutableCommandline>%options% - -o "{0}" </ExecutableCommandline>
				<TitleFormatString>OGG Vorbis OggEnc2 - {0}</TitleFormatString>
				<SupportedFileExtension>ogg</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<Radiobutton Name="ABR mode {0} kb/s">
					<Value>--bitrate {0}</Value>
					<Trackbar DefaultValue="130">
						<Min>8</Min>
						<Max>496</Max>
						<TickFrequency>36</TickFrequency>
					</Trackbar>
					<ToolTip>Average Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="VBR mode Quality: {0:f2}">
					<Value>--quality {0:f2}</Value>
					<Trackbar DefaultValue="300">
						<Min>-200</Min>
						<Max>1000</Max>
						<TickFrequency>100</TickFrequency>
						<Multiplier>0.01</Multiplier>
					</Trackbar>
					<ToolTip>Variable Bitrate mode</ToolTip>
				</Radiobutton>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>