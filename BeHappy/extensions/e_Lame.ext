<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="51388149-93da-42ed-8342-25c1f6af18aa">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>370</DialogWidth>
				<Url>http://lame.sourceforge.net/links.php#Binaries</Url>
				<UrlToolTip>Encoder documentation and download.</UrlToolTip>
				<LogoBitmap></LogoBitmap>
				<Script>32==Audiobits(last)?ConvertAudioTo32bit(last):last</Script>			<!-- Better an error than a wrong result when multichannel -->
				<ExecutableFileName>lame.exe</ExecutableFileName>
				<ExecutableCommandline>%options% --brief - "{0}"</ExecutableCommandline>
				<TitleFormatString>Lame MP3 - {0}</TitleFormatString>
				<SupportedFileExtension>mp3</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<Radiobutton Name="CBR mode {0} kb/s">
					<Value>--cbr -b {0}</Value>
					<Trackbar DefaultValue="8" FixedValues="true">
						<Value>32</Value>
						<Value>40</Value>
						<Value>48</Value>
						<Value>56</Value>
						<Value>64</Value>
						<Value>80</Value>
						<Value>96</Value>
						<Value>112</Value>
						<Value>128</Value>
						<Value>160</Value>
						<Value>192</Value>
						<Value>224</Value>
						<Value>256</Value>
						<Value>320</Value>
					</Trackbar>
					<ToolTip>Constant Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="ABR mode {0} kb/s">
					<Value>--abr {0}</Value>
					<Trackbar DefaultValue="128">
						<Min>8</Min>
						<Max>320</Max>
						<TickFrequency>12</TickFrequency>
					</Trackbar>
					<ToolTip>Average Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="VBR mode Quality: {0}" Default="true">
					<Value>-V {0}</Value>
					<Trackbar DefaultValue="4" FixedValues="true">
					<Value>0</Value>
					<Value>1</Value>
					<Value>2</Value>
					<Value>3</Value>
					<Value>4</Value>
					<Value>5</Value>
					<Value>6</Value>
					<Value>7</Value>
					<Value>8</Value>
					<Value>9</Value>
					</Trackbar>
					<ToolTip>Variable Bitrate mode\n\n0 = High quality, bigger files\n4 = default\n9 = low quality, smaller files</ToolTip>
				</Radiobutton>
				<Dropdown Name="Channel Mode">
					<Item Name="Automatic">
						<Value></Value></Item>
					<Item Name="Joint Stereo">
						<Value>-m j</Value></Item>
					<Item Name="MS Stereo">
						<Value>-m f</Value></Item>
					<Item Name="LR Stereo">
						<Value>-m s</Value></Item>
					<Item Name="Dual Mono">
						<Value>-m d</Value></Item>
					<Item Name="Mono">
						<Value>-m m</Value></Item>
					<Item Name="Left">
						<Value>-m l</Value></Item>
					<Item Name="Right">
						<Value>-m r</Value></Item>
					<ToolTip>Force Channel Mode\n\nAutomatic = Joint or LR depending on bitrate.</ToolTip>
				</Dropdown>
				<NumericUpdown Name="Quality" DefaultValue="5">
					<Value>-q {0}</Value>
					<Min>0</Min>
					<Max>9</Max>
					<DecimalPlaces>0</DecimalPlaces>
					<Increment>1</Increment>
					<ToolTip>0 = Highest quality, slow\n5 = Default\n9 = Poor quality, fast</ToolTip>
				</NumericUpdown>
				<Checkbox Name="old VBR routine">
					<ValueChecked>--vbr-old</ValueChecked>
					<ToolTip>Use the old VBR routine instead of the new one.</ToolTip>
				</Checkbox>
				<Checkbox Name="Copyright">
					<ValueChecked>-c</ValueChecked>
					<ToolTip>Mark as copyright</ToolTip>
				</Checkbox>
				<Checkbox Name="Non-original">
					<ValueChecked>-o</ValueChecked>
					<ToolTip>Mark as non-original</ToolTip>
				</Checkbox>
				<Checkbox Name="Error protection">
					<ValueChecked>-p</ValueChecked>
					<ToolTip>Error protection. Adds 16 bit checksum to every frame (the checksum is computed correctly).</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>