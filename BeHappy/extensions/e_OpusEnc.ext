<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="49371dae-70bb-460c-b6f7-7d1cba00d387">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>370</DialogWidth>
				<LogoBitmap></LogoBitmap>
				<Url>https://www.opus-codec.org</Url>
				<UrlToolTip>Encoder documentation and download.</UrlToolTip>
				<Script></Script>
				<ExecutableFileName>opusenc.exe</ExecutableFileName>
				<ExecutableCommandline>%options% --ignorelength - "{0}"</ExecutableCommandline>
				<TitleFormatString>OpusEnc - {0}</TitleFormatString>
				<SupportedFileExtension>opus</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<UseRawPCM>false</UseRawPCM>
				<Radiobutton Name="CBR mode {0} kb/s">
					<Value>--hard-cbr --bitrate {0}</Value>
					<Trackbar DefaultValue="130">
						<Min>6</Min>
						<Max>510</Max>
						<TickFrequency>32</TickFrequency>
					</Trackbar>
					<ToolTip>Constant Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="CVBR mode {0} kb/s">
					<Value>--cvbr --bitrate {0}</Value>
					<Trackbar DefaultValue="130">
						<Min>6</Min>
						<Max>510</Max>
						<TickFrequency>32</TickFrequency>
					</Trackbar>
					<ToolTip>Constrained Variable Bitrate mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="VBR mode {0}" Default="true">
					<Value>--vbr --bitrate {0}</Value>
					<Trackbar DefaultValue="130">
						<Min>6</Min>
						<Max>510</Max>
						<TickFrequency>32</TickFrequency>
					</Trackbar>
					<ToolTip>Variable Bitrate mode</ToolTip>
				</Radiobutton>
				<NumericUpdown Name="Complexity" DefaultValue="10">
					<Value>--comp {0}</Value>
					<Min>0</Min>
					<Max>10</Max>
					<ToolTip>Encoding complexity (default: 10 (slowest))</ToolTip>
				</NumericUpdown>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>	
</BeHappy.Extension>