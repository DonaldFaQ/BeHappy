<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="ef2e1e02-56a9-4ad7-9ca7-5992fe37bd34">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>350</DialogWidth>
				<LogoBitmap></LogoBitmap>
				<Script>32==Audiobits(last)?ConvertAudioTo24bit(last):last</Script>
				<UseRawPCM>true</UseRawPCM>
				<ExecutableFileName>mp3sencoder.exe</ExecutableFileName>
				<ExecutableCommandline>-if - -of "{0}" -sr {1} -res {2} -c {3} -eof %options%</ExecutableCommandline>
				<TitleFormatString>FHG MP3 - {0}</TitleFormatString>
				<SupportedFileExtension>mp3</SupportedFileExtension>
				<Radiobutton Name="VBR Mode Quality: {0}">
					<Value>-vbri -br 0 -m {0}</Value>
					<Trackbar DefaultValue="3">
						<Min>1</Min>
						<Max>5</Max>
						<TickFrequency>1</TickFrequency>
					</Trackbar>
					<ToolTip>Variable Bitrate Mode\n\n1 = Highest Quality\n2 = High Quality\n3 = Intermediate Quality\n4 = Intermediate Bitrate\n5 = Low Bitrate</ToolTip>
				</Radiobutton>
				<Radiobutton Name="CBR Mode {0} kb/s">
					<Value>-br {0}000</Value>
					<Trackbar DefaultValue="8" FixedValues="true">
						<Value>32</Value>
						<Value>40</Value>
						<Value>48</Value>
						<Value>56</Value>
						<Value>64</Value>
						<Value>80</Value>
						<Value>96</Value>
						<Value>112</Value>
						<Value>128</Value>
						<Value>160</Value>
						<Value>192</Value>
						<Value>224</Value>
						<Value>256</Value>
						<Value>320</Value>
					</Trackbar>
					<ToolTip>Constant Bitrate Mode\nValid Range:\nMono Mp3: 16-256 Kb/s\nStereo Mp3: 96-320 Kb/s\nMp3 Sorrounds: 128-320 Kb/s </ToolTip>
				</Radiobutton>
				<Checkbox Name="High Quality" DefaultChecked="true">
					<ValueChecked>-q 1</ValueChecked>
					<ValueUnChecked>-q 0</ValueUnChecked>
					<ToolTip>Use high quality mode instead of fast mode.</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>
		</Plugin>
	</AudioEncoder>
</BeHappy.Extension>
