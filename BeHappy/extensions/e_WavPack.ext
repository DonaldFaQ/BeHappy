<?xml version="1.0"?>
<BeHappy.Extension xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://workspaces.gotdotnet.com/behappy">
	<AudioEncoder UniqueID="CA5FD3A2-638D-4D73-951F-1B745B02C1C5">
		<Plugin>
			<MultiOptionEncoder Type="BeHappy.Extensions.MultiOptionEncoder, BeHappy">
				<DialogWidth>450</DialogWidth>
				<Url>http://www.wavpack.com/</Url>
				<LogoBitmap></LogoBitmap>
				<ExecutableFileName>wavpack.exe</ExecutableFileName>
				<ExecutableCommandline>%options% -i -l -m -r -q -y - "{0}"</ExecutableCommandline>
				<TitleFormatString>WV Wavpack - {0}</TitleFormatString>
				<SupportedFileExtension>wv</SupportedFileExtension>
				<ShowCommandTextbox>true</ShowCommandTextbox>
				<Radiobutton Name="Fast Mode">
					<Value>-f</Value>
					<ToolTip>Faster encode and decode, but some compromise in compression ratio</ToolTip>
				</Radiobutton>
				<Radiobutton Name="High Quality" Default="true">
					<Value>-h</Value>
					<ToolTip>Better compression ratio, but slower encode and decode than default mode</ToolTip>
				</Radiobutton>
				<Radiobutton Name="Very High Quality">
					<Value>-hh</Value>
					<ToolTip>Best compression, but slowest and NOT recommended for portable hardware use</ToolTip>
				</Radiobutton>
				<Radiobutton Name="Hybrid compression {0} kb/s">
					<Value>-b{0} -c</Value>
					<Trackbar DefaultValue="256">
						<Min>24</Min>
						<Max>9600</Max>
						<TickFrequency>240</TickFrequency>
					</Trackbar>
					<ToolTip></ToolTip>
				</Radiobutton>
				<Checkbox Name="LossyWV">
					<ValueChecked>--blocksize=512 --merge-blocks</ValueChecked>
					<ToolTip>Set to frame size limit to 512k samples for perchannels</ToolTip>
				</Checkbox>
				<Checkbox Name="Extra encode processing">
					<ValueChecked>-x3</ValueChecked>
					<ToolTip>This is enabled with the -x option and provides an average improvement in CD music compression 
					of about 1% in "fast" mode, about 0.5% in the normal mode, and still less in the higher modes.</ToolTip>
				</Checkbox>
			</MultiOptionEncoder>  
		</Plugin>
	</AudioEncoder>
</BeHappy.Extension>
